for i in range(len(exp)):
    if exp[i] in ['(','{','[']:
        s.push(exp(i))
    elif exp[i] in [')','}',']']:
        if pair(exp[i]) == s.pop():
            continue
        else:
            return False
    if s.isEmpty():
        return True


